<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\Blizzard_SharedXML\UI.xsd">
	<Frame name="GlueGameLogoTemplate" mixin="GlueGameLogoMixin" virtual="true">
		<KeyValues>
			<KeyValue key="useShrunkenLogoHeight" value="false" type="boolean"/>
		</KeyValues>
		<Layers>
			<Layer level="ARTWORK">
				<Texture parentKey="LogoTexture">
					<Size x="256" y="128"/>
					<Anchors>
						<Anchor point="TOP"/>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
		<Scripts>
			<OnLoad method="OnLoad"/>
			<OnShow method="OnShow"/>
			<OnEvent method="OnEvent"/>
		</Scripts>
	</Frame>
</Ui>
